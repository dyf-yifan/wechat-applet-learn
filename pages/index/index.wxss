.swiper {
  height: 400rpx;
  width: 100%;
}

.swiper image {
  height: 100%;
  width: 100%;
}

.d-top {
  margin: 0 auto;
  width: 96%;
  height: 90rpx;
  display: flex;
  align-items: center;
}

.d-img {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.d-title {
  margin-left: 3%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.d-icon {
  margin-left: 40%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.d-imgl {
  width: 75rpx;
  height: 75rpx;
  border-radius: 50%;
}

.d-iconl {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
}

.d-my {
  margin: 30rpx 10rpx 30rpx 35rpx;
  width: 90%;
  height: 200rpx;
  box-shadow: beige;
  -webkit-box-shadow: 4px 2px 6px 2px #DCDCDC;
  -moz-box-shadow: 4px 2px 6px 2px #DCDCDC;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 7px;
}

.d-my-left {
  width: 20%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.d-my-right {
  width: 60%;
  height: 60%;
}

.d-my-right-top {
  width: 100%;
  height: 40%;
  display: flex;
}

.d-my-right-bottom {
  width: 100%;
  height: 40%;
  display: flex;
  font-size: 30rpx;
}

.d-my-right-bottom {
  display: flex;
  justify-content: space-around;
}

.d-my-right-name {
  margin-left: 15rpx;
}

.d-my-right-name-l {
  color: darkgray;
  font-size: 30rpx;
  margin-left: 20rpx;
}

.d-my-rightl {
  width: 23%;
  height: 70%;
  margin-right: 15rpx;
}

.d-img-my {
  width: 95rpx;
  height: 95rpx;
  border-radius: 50%;
}

.d-youhui {
  display: flex;
  justify-content: space-between;
  width: 95%;
  margin: 0 auto;
}

.d-card {
  display: flex;
  margin: 30rpx 10rpx 30rpx 35rpx;
  width: 90%;
  justify-content: space-around;
}

.d-youhui-card {
  width: 31%;
  height: 320rpx;
  box-shadow: beige;
  -webkit-box-shadow: 4px 2px 6px 2px #DCDCDC;
  -moz-box-shadow: 4px 2px 6px 2px #DCDCDC;
  border-radius: 9px;
}

.d-yh-img {
  width: 100%;
  height: 40%;
}

.d-button {
  margin-top: 4%;
  width: 60%;
  height: 16%;
  font-size: 20rpx;
  color: rgb(255, 255, 255);
  background-color: rgb(255 69 0);
  border-radius: 15px;
}

.d-buttonl {
  margin-top: 4%;
  width: 60%;
  height: 16%;
  font-size: 20rpx;
  color: rgb(255, 255, 255);
  background-color: rgb(204, 208, 219);
  border-radius: 15px;
}

.d-bing {
  margin-top: 10rpx;
  font-size: 35rpx;
  width: 80%;
  height: 40%;
}

.d-jingxuan {
  width: 90%;
  margin: 30rpx 10rpx 30rpx 35rpx;
}

.d-jingxuan-top {
  width: 100%;
  height: 180rpx;
  display: flex;
  justify-content: space-around;
}

.d-jx-img {
  width: 48%;
  border-radius: 11rpx;
}

.d-jingxuan-bottom {
  margin-top: 10rpx;
  width: 100%;
  flex-wrap: wrap;
  display: flex;
  justify-content: space-around;
}

.d-jx-card {
  width: 48%;
  height: 130rpx;
  border-radius: 11rpx;
  display: flex;
  box-shadow: beige;
  -webkit-box-shadow: 4px 2px 6px 2px #DCDCDC;
  -moz-box-shadow: 4px 2px 6px 2px #DCDCDC;
  margin-top: 18rpx;
}

.d-jx-card-left {
  width: 70%;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  margin-left: 10rpx;
}

.d-jx-card-right {
  width: 30%;
  height: 100%;
  margin-left: 5%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.d-jx-icon {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}

.d-jx-card-name {
  width: 100%;
  height: 50%;
}

.d-jx-card-content {
  width: 100%;
  height: 50%;
  color: darkgray;
  font-size: 30rpx;
}

.d-bottom {
  display: flex;
  justify-content: center;
  color: darkgray;
}